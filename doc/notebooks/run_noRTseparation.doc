# {{{
#
wget http://www.peptideatlas.org/builds/human/201111/APD_Hs_all.fasta
grep -c ">" APD_Hs_all.fasta 
grep -v ">" APD_Hs_all.fasta > APD_Hs_peptides.fasta
perl SSRCalc3.pl --alg 3.0 --source_file APD_Hs_peptides.fasta  --output tsv --B 1 --A 0  > APD_Hs_peptides.ssrcalc
grep -v Z scripts/APD_Hs_peptides.ssrcalc | grep -v B > scripts/APD_Hs_peptides.fix.ssrcalc
python scripts/create_db.py --mysql_config=~/.my.cnf.srmcollider --peptide_table=srmcollider.human_pa --tsv_file=scripts/APD_Hs_peptides.fix.ssrcalc

